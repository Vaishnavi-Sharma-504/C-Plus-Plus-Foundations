#include <iostream>
using namespace std;
int main()
{
    int a, b;
    cout<<"Enter first no.:";
    cin>>a;
    cout<<"Enter second no.:";
    cin>>b;
    if (a>b)
    cout<<"The larger no. is:"<<a;
    else if (b>a)
    cout<<"The larger no. is:"<<b;
    else
    cout<<"Both no. are equal";
    return 0;
    
}

